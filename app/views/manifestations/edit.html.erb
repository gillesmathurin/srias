<div class="span-16">
	<h1>Modifier Infos Manifestation</h1>

	<% form_for(@manifestation, :html => {:multipart => true}) do |f| %>
	  <%= f.error_messages %>

		<%= render :partial => 'form', :locals => {:f => f} %>

	  <p>
	    <%= f.submit 'Modifier' %>
	  </p>
	<% end %>

	<%= link_to 'Voir', @manifestation %> |
	<%= link_to 'Retour', manifestations_path %>
</div>
<!-- Right Sidebar -->
<div class="span-4 last">
	<%= render :partial => "shared/right_col" %>
</div>
