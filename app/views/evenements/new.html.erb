<div class="span-16">
<h1>Enregistrer un Evenement</h1>

	<% form_for(@evenement) do |f| %>
	  <%= f.error_messages %>

	<%= render :partial => 'form', :locals => {:f => f} %>

	  <p>
	    <%= f.submit 'Enregistrer' %>
	  </p>
	<% end %>

	<%= link_to 'Retour', evenements_path %>
</div>
<!-- Right Sidebar -->
<div class="span-4 last">
	<%= render :partial => "shared/right_col" %>
</div>