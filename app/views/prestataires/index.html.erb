<h1>Listing prestataires</h1>

<table>
  <tr>
    <th>Nom</th>
    <th>Website</th>
    <th>Tel</th>
    <th>Mob</th>
    <th>Fax</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prestataires.each do |prestataire| %>
  <tr>
    <td><%= prestataire.nom %></td>
    <td><%= prestataire.website %></td>
    <td><%= prestataire.tel %></td>
    <td><%= prestataire.mob %></td>
    <td><%= prestataire.fax %></td>
    <td><%= prestataire.description %></td>
    <td><%= link_to 'Show', prestataire %></td>
    <td><%= link_to 'Edit', edit_prestataire_path(prestataire) %></td>
    <td><%= link_to 'Destroy', prestataire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prestataire', new_prestataire_path %>
