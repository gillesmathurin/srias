<div class="span-16">
	<h1 class="section-title">Nouveau Lien</h1>

	<%= form_for(@lien) do |f| %>
		<%= render :partial => 'shared/form_errors', :locals => {:object => @lien} %>

		<%= render :partial => 'form', :locals => {:f => f} %>

	  <p>
	    <%= f.submit 'Enregistrer', class: 'btn' %>
	  </p>
	<% end %>

	<%= link_to 'Retour', liens_path, :class => "btn btn-sm" %>
</div>