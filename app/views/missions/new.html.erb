<div class="span-16">
	<h1 class="section-title">Nouvelle Mission</h1>

	<%= form_for(@mission) do |f| %>
	  	<%= render :partial => 'shared/form_errors', :locals => {:object => @mission} %>

		<%= render :partial => 'form', :locals => {:f => f} %>

	  <p><%= f.submit 'Enregistrer' %></p>
	<% end %>

	<%= link_to 'Retour', missions_path, :class =>"btn btn-sm" %>
</div>

